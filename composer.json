{
  "name": "FranceMediasMonde/behat-cucumber-json-formatter",
  "description": "Behat extension for generating json reports",
  "keywords": ["behat", "json", "formatter", "cucumber"],
  "homepage": "https://git.fmm.adm.fr.clara.net/FranceMediasMonde/behat-cucumber-formatter",
  "authors": [
    {
      "name": "FranceMediasMonde",
      "email": "grp_dev_mezzo@francemm.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=5.5.0",
    "behat/behat": "~3.0"
  },
  "minimum-stability": "dev",
  "config": {
    "bin-dir": "bin/"
  },
  "require-dev": {
    "mikey179/vfsStream": "1.6.0",
    "phpunit/phpunit": "^4"
  },
  "autoload": {
    "psr-4": {
      "FranceMediasMonde\\BehatCucumberJsonFormatter\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "FranceMediasMonde\\BehatCucumberJsonFormatter\\Tests\\": "tests/"
    }
  }
}
